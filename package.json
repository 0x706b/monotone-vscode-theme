{
   "name": "monotone-vscode-theme",
   "displayName": "monotone",
   "description": "An aesthetically-pleasing utilitarian theme, forked from Lokaltog/vim-monotone",
   "version": "0.0.1",
   "engines": {
      "vscode": "^1.44.0"
   },
   "scripts": {
      "watch": "tsnd --respawn ./src/monotone.ts",
      "build": "tsnd ./src/monotone.ts"
   },
   "categories": [
      "Themes"
   ],
   "publisher": "0x706b",
   "icon": "./icon.png",
   "contributes": {
      "themes": [
         {
            "label": "monotone",
            "uiTheme": "vs-dark",
            "path": "./themes/monotone-color-theme.json"
         }
      ]
   },
   "dependencies": {},
   "devDependencies": {
      "@types/node": "^13.13.0",
      "@types/vscode": "^1.44.0",
      "ts-node-dev": "^1.0.0-pre.44",
      "typescript": "^3.8.3"
   }
}
